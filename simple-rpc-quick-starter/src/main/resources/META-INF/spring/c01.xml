<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:simple="http://www.simple.com/schema/simple"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd
        http://www.simple.com/schema/simple
       http://www.simple.com/schema/simple.xsd"
>

    <simple:boot/>
    <simple:node application="quick-start-02" port="8890"/>
    <simple:register address="192.168.1.110:2181" protocol="zookeeper"/>


    <simple:reference id="app2Impl" interface="com.simple.quick.starter.spring.App2" version="2.0"/>
    <simple:reference id="app3Impl" interface="com.simple.quick.starter.spring.App3" version="2.0"/>


</beans>